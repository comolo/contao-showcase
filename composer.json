{
  "name": "comolo/contao-showcase",
  "type": "contao-bundle",
  "description": "Adds showcase functionality to Contao 4",
  "license": "proprietary",
  "authors": [
    {
      "name": "Hendrik Obermayer",
      "homepage": "https://github.com/henobi"
    }
  ],
  "require": {
    "php": "^7.0",
    "contao/core-bundle": "^4.4",
    "symfony/framework-bundle": "^3.4 || ^4.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "extra": {
    "contao-manager-plugin": "Comolo\\ShowcaseBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Comolo\\ShowcaseBundle\\": "src/"
    }
  }
}
